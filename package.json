		"gh-pages": "^5.0.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.2",
		"lint-staged": "^13.2.2",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^5.4.4",
		"vite-node": "^1.6.0",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"clsx": "^2.1.1"
	},
